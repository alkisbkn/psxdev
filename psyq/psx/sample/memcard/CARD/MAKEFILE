# Note:
# This Makefile is for GNU compiler in UNIX
# You have to change this for PC environment. Sorry.
#
# 'makdfile.mak' is a sample Makefile in PC environment.
# It must help you to convert this Makefile
#
LIBDIR   = ../../../lib
INCDIR   = ../../../include
MAKE	 = make
MAKE	 = make
TADDR	 = 80010000
CC       = cc -EL -G 0 -O2
CFLAGS   = -I$(INCDIR)
RM       = rm -f
RM_CMD   = $(RM) *.CKP *.ln *.BAK *.bak *.o core errs ,* *~ *.a .emacs_* tags TAGS make.log MakeOut
DEPEND   = makedepend
LIBS     = \
	   $(LIBDIR)/libgpu.a \
	   $(LIBDIR)/libgte.a \
	   $(LIBDIR)/libetc.a \
	   $(LIBDIR)/libcard.a \
	   $(LIBDIR)/libc.a \
	   $(LIBDIR)/libapi.a

OBJS     = card.o

all::	main
main : $(OBJS)
	ld -EL -N -T $(TADDR) -e main -o main $(OBJS) $(LIBS)

clean::
	$(RM) main
	$(RM_CMD) "#"*

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(CFLAGS) -- *.c


load:
	psx smon
	sleep 1
	aload main
	squit

start:
	psx ""
	make load
	psx "sr sp 807ffff0"
	psx "call"

# -------------------------------------------------------------------------
# dependencies generated by makedepend

# DO NOT DELETE

main.o: ../../../include/sys/types.h ../../../include/libgpu.h
main.o: ../../../include/libetc.h balltex.h
